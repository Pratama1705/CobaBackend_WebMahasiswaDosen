<form method="POST" action="/add-matkul-dosen">
  <div class="form-group">
    <label for="emailInput">ID Matakuliah</label>
    <input name="idMatkul" type="text" class="form-control" id="emailInput" placeholder="ID Matakuliah" />
  </div>

  <div class="form-group">
    <label for="passwordInput">Nama Matakuliah</label>
    <input name="namaMatkul" type="text" class="form-control" id="passwordInput" placeholder="Nama Matakuliah" />
  </div>
  <button type="submit" class="btn btn-primary">Add</button>
</form>